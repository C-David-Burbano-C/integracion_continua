{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/assets/cesium/(.*)",
      "destination": "/assets/cesium/$1"
    }
  ],
  "headers": [
    {
      "source": "/assets/cesium/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}